# API Key (required for STDIO mode, optional for HTTP modes if OAuth is configured)
APIKEY=your-octomind-api-key

# OAuth Configuration (optional - enables OAuth 2.1 authentication)
# If not set, server uses API key authentication only
OAUTH_AUTH_SERVER_URL=https://6393987891.propelauthtest.com
SERVER_BASE_URL=https://octomind.me:7100/
OAUTH_SCOPES=octomind:read,octomind:write

# Redis Configuration (optional - for session/event storage)
#REDIS_URL=redis://localhost:6379
#REDIS_SESSION_STORE_ENABLED=false
#REDIS_EVENT_STORE_ENABLED=false

# Server Configuration
PORT=3002
